<Window x:Class="Front_layer.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Front_layer"
        mc:Ignorable="d"
        Title="MainWindow" Height="150" Width="300">
    <Grid>
        <Label x:Name="lblGebruiker" Content="Uw gebruikersnaam" HorizontalAlignment="Left" Margin="25,40,0,0" VerticalAlignment="Top"/>
        <TextBox x:Name="tbGebruiker" HorizontalAlignment="Left" Height="23" Margin="145,44,0,0" TextWrapping="Wrap" Text="" VerticalAlignment="Top" Width="120"/>
        <Label x:Name="lblwachtwoord" Content="Uw wachtwoord" HorizontalAlignment="Left" Margin="25,71,0,0" VerticalAlignment="Top"/>
        <PasswordBox x:Name="tbwachtwoord" HorizontalAlignment="Left" Height="23" Margin="145,75,0,0" VerticalAlignment="Top" Width="120"/>
        <RadioButton x:Name="rb1" Content="Gebruiker" HorizontalAlignment="Left" Margin="39,10,0,0" VerticalAlignment="Top" IsChecked="True"/>
        <RadioButton x:Name="rb2" Content="Bank" HorizontalAlignment="Left" Margin="145,10,0,0" VerticalAlignment="Top" IsChecked="False"/>
    </Grid>
</Window>
